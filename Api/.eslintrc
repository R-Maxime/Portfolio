{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "Api/tsconfig.json"
  },
  "rules": {
    "no-console": "off",
    "no-new": "off",
    "no-continue": "off",
    "max-len": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off"
  }
}